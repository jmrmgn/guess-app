    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.meep26.guess_app",
      "versionCode": 1,
      "permissions": ["WRITE_INTERNAL_STORAGE"]
    }
  }
}
